{"ast":null,"code":"import modal from \"@/components/Modal.vue\";\nimport Alert from \"@/components/Alert.vue\";\nimport Card from \"@/components/Card.vue\";\nimport { initFlowbite } from \"flowbite\";\nimport Weight1 from \"@/components/weightStation/weight1.vue\";\nimport Weight2 from \"@/components/weightStation/weight2.vue\";\nexport default {\n  name: \"weightStationPanel\",\n  components: {\n    Weight2,\n    Weight1,\n    modal,\n    Alert,\n    Card\n  },\n  data() {\n    return {\n      btnPanel: true,\n      btns: [{\n        name: 'وزن اولیه',\n        isActive: false\n      }, {\n        name: 'وزن ثانویه',\n        isActive: false\n      }]\n    };\n  },\n  methods: {\n    activePanel(panel) {\n      for (let i = 0; i < this.btns.length; i++) {\n        if (panel.name == this.btns[i].name) {\n          this.btns[i].isActive = true;\n          this.btnPanel = false;\n        } else {\n          this.btns[i].isActive = false;\n        }\n      }\n    },\n    backToPanel() {\n      for (let i = 0; i < this.btns.length; i++) {\n        if (this.btns[i].isActive) {\n          this.btns[i].isActive = false;\n          this.btnPanel = true;\n        }\n      }\n    }\n  },\n  mounted() {\n    initFlowbite();\n  }\n};","map":{"version":3,"names":["modal","Alert","Card","initFlowbite","Weight1","Weight2","name","components","data","btnPanel","btns","isActive","methods","activePanel","panel","i","length","backToPanel","mounted"],"sources":["C:\\Users\\Amir\\ISMv988\\ISMv900\\frontend\\src\\components\\weightStation\\weightStationPanel.vue"],"sourcesContent":["<script>\r\nimport modal from \"@/components/Modal.vue\";\r\nimport Alert from \"@/components/Alert.vue\";\r\nimport Card from \"@/components/Card.vue\";\r\nimport {initFlowbite} from \"flowbite\";\r\nimport Weight1 from \"@/components/weightStation/weight1.vue\";\r\nimport Weight2 from \"@/components/weightStation/weight2.vue\";\r\n\r\nexport default {\r\n  name: \"weightStationPanel\",\r\n  components: {\r\n    Weight2,\r\n    Weight1,\r\n    modal,\r\n    Alert,\r\n    Card,\r\n  },\r\n  data(){\r\n    return {\r\n      btnPanel: true,\r\n      btns: [\r\n          {\r\n            name: 'وزن اولیه',\r\n            isActive: false\r\n          },\r\n          {\r\n            name: 'وزن ثانویه',\r\n            isActive: false\r\n          },\r\n      ]\r\n    }\r\n  },\r\n  methods:{\r\n    activePanel(panel){\r\n      for (let i = 0; i < this.btns.length; i++) {\r\n          if (panel.name == this.btns[i].name){\r\n            this.btns[i].isActive = true\r\n            this.btnPanel = false\r\n          } else {\r\n            this.btns[i].isActive = false\r\n          }\r\n      }\r\n    },\r\n    backToPanel(){\r\n      for (let i = 0; i < this.btns.length; i++) {\r\n          if (this.btns[i].isActive) {\r\n            this.btns[i].isActive = false\r\n            this.btnPanel = true\r\n          }\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    initFlowbite();\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n  <Card v-if=\"btnPanel\" title=\"پنل ایستگاه وزن کشی\">\r\n    <div class=\"flex flex-col gap-2 justify-center items-center\">\r\n      <button v-for=\"btn in btns\" @click=\"activePanel(btn)\" type=\"button\" class=\"inline-flex justify-center w-48 px-2 py-1.5 text-sm font-medium text-center rounded-lg text-white   bg-green-500 hover:bg-green-600 focus:ring-4 focus:outline-none focus:ring-green-300\">\r\n        {{btn.name}}\r\n      </button>\r\n    </div>\r\n  </Card>\r\n  <template v-else>\r\n    <template v-for=\"panel in btns\">\r\n      <template v-if=\"panel.isActive==true\">\r\n        <Card :title=\"panel.name\" v-if=\"panel.name=='وزن اولیه'\">\r\n          <div class=\"flex flex-col gap-2 justify-center items-center\">\r\n            <weight1></weight1>\r\n            <button @click=\"backToPanel\" type=\"button\" class=\"inline-flex justify-center w-48 px-2 py-1.5 text-sm font-medium text-center text-white bg-rose-600 rounded-lg hover:bg-rose-700 focus:ring-4 focus:outline-none focus:ring-rose-300 dark:bg-rose-500 dark:hover:bg-rose-600 dark:focus:ring-rose-800\">\r\n              بازگشت به پنل\r\n            </button>\r\n          </div>\r\n        </Card>\r\n        <Card :title=\"panel.name\" v-if=\"panel.name=='وزن ثانویه'\">\r\n          <div class=\"flex flex-col gap-2 justify-center items-center\">\r\n            <weight2></weight2>\r\n            <button @click=\"backToPanel\" type=\"button\" class=\"inline-flex justify-center w-48 px-2 py-1.5 text-sm font-medium text-center text-white bg-rose-600 rounded-lg hover:bg-rose-700 focus:ring-4 focus:outline-none focus:ring-rose-300 dark:bg-rose-500 dark:hover:bg-rose-600 dark:focus:ring-rose-800\">\r\n              بازگشت به پنل\r\n            </button>\r\n          </div>\r\n        </Card>\r\n      </template>\r\n    </template>\r\n  </template>\r\n</template>\r\n"],"mappings":"AACA,OAAOA,KAAI,MAAO,wBAAwB;AAC1C,OAAOC,KAAI,MAAO,wBAAwB;AAC1C,OAAOC,IAAG,MAAO,uBAAuB;AACxC,SAAQC,YAAY,QAAO,UAAU;AACrC,OAAOC,OAAM,MAAO,wCAAwC;AAC5D,OAAOC,OAAM,MAAO,wCAAwC;AAE5D,eAAe;EACbC,IAAI,EAAE,oBAAoB;EAC1BC,UAAU,EAAE;IACVF,OAAO;IACPD,OAAO;IACPJ,KAAK;IACLC,KAAK;IACLC;EACF,CAAC;EACDM,IAAIA,CAAA,EAAE;IACJ,OAAO;MACLC,QAAQ,EAAE,IAAI;MACdC,IAAI,EAAE,CACF;QACEJ,IAAI,EAAE,WAAW;QACjBK,QAAQ,EAAE;MACZ,CAAC,EACD;QACEL,IAAI,EAAE,YAAY;QAClBK,QAAQ,EAAE;MACZ,CAAC;IAEP;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,WAAWA,CAACC,KAAK,EAAC;MAChB,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,IAAI,CAACL,IAAI,CAACM,MAAM,EAAED,CAAC,EAAE,EAAE;QACvC,IAAID,KAAK,CAACR,IAAG,IAAK,IAAI,CAACI,IAAI,CAACK,CAAC,CAAC,CAACT,IAAI,EAAC;UAClC,IAAI,CAACI,IAAI,CAACK,CAAC,CAAC,CAACJ,QAAO,GAAI,IAAG;UAC3B,IAAI,CAACF,QAAO,GAAI,KAAI;QACtB,OAAO;UACL,IAAI,CAACC,IAAI,CAACK,CAAC,CAAC,CAACJ,QAAO,GAAI,KAAI;QAC9B;MACJ;IACF,CAAC;IACDM,WAAWA,CAAA,EAAE;MACX,KAAK,IAAIF,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,IAAI,CAACL,IAAI,CAACM,MAAM,EAAED,CAAC,EAAE,EAAE;QACvC,IAAI,IAAI,CAACL,IAAI,CAACK,CAAC,CAAC,CAACJ,QAAQ,EAAE;UACzB,IAAI,CAACD,IAAI,CAACK,CAAC,CAAC,C