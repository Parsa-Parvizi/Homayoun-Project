{"ast":null,"code":"import { initFlowbite } from \"flowbite\";\nimport modal from \"@/components/Modal.vue\";\nimport Alert from \"@/components/Alert.vue\";\nimport Card from \"@/components/Card.vue\";\nimport ModalButton from \"@/components/custom/ModalButton.vue\";\nimport Input from \"@/components/custom/Input.vue\";\nimport Dropdown from \"@/components/custom/Dropdown.vue\";\nexport default {\n  name: \"unloaded\",\n  components: {\n    Dropdown,\n    Input,\n    ModalButton,\n    Card,\n    Alert,\n    modal\n  },\n  data() {\n    return {\n      forms: {\n        lic_number: {\n          type: 'dropdown',\n          name: 'شماره پلاک',\n          title: 'شماره پلاک',\n          data: '',\n          value: ''\n        },\n        unloading_location: {\n          type: 'dropdown',\n          name: 'محل تخلیه',\n          title: 'محل تخلیه',\n          data: '',\n          value: ''\n        },\n        unit: {\n          type: 'dropdown',\n          name: 'واحد',\n          title: 'واحد',\n          data: '',\n          value: ''\n        },\n        Quantity: {\n          type: 'input',\n          name: 'مقدار',\n          title: 'مقدار',\n          value: '',\n          numbertype: true,\n          lable: 'number'\n        },\n        Quality: {\n          type: 'input',\n          name: 'کیفیت',\n          title: 'کیفیت',\n          value: '',\n          numbertype: true\n        },\n        forklift_driver: {\n          type: 'input',\n          name: 'اسم راننده لیفت تراک',\n          title: 'اسم راننده لیفت تراک',\n          value: ''\n        }\n      },\n      success: false,\n      error: false,\n      errors: []\n    };\n  },\n  computed: {\n    formattedValue() {\n      return this.formatNumber(this.inputValue);\n    }\n  },\n  mounted() {\n    initFlowbite();\n    const params = {\n      \"status\": 'LoadingUnloading',\n      \"location\": 'Weight1',\n      \"shipment_type\": 'Incoming'\n    };\n    this.axios.post('/myapp/api/getShipmentLicenseNumbers', {}, {\n      params: params\n    }).then(response => {\n      console.log('lics:', response.data);\n      this.forms.lic_number.data = response.data['license_numbers'];\n    });\n    this.axios.get('/myapp/api/getAnbarTableNames').then(response => {\n      console.log(response.data);\n      this.forms.unloading_location.data = response.data['data'];\n    });\n  },\n  watch: {\n    success(c, p) {\n      if (c == true) {\n        setTimeout(() => {\n          this.success = false;\n          location.reload();\n        }, 5000);\n      }\n    }\n  },\n  methods: {\n    formatNumber(value) {\n      return value.replace(/\\D/g, \"\").replace(/\\B(?=(\\d{3})+(?!\\d))/g, \",\");\n    },\n    formatInput() {\n      this.forms.Quantity.value = this.formatNumber(this.forms.Quantity.value);\n      // this.forms.Quality.value = this.formatNumber(this.forms.Quality.value);\n    },\n    clicked(k, name) {\n      console.log(k, name);\n      if (k == 'lic_number') {\n        this.forms.lic_number.name = name;\n        this.forms.lic_number.value = name;\n        const params = {\n          'lic_number': this.forms.lic_number.value\n        };\n        this.axios.get('/myapp/api/getUnitNamesBasedOnLicenseOfShipment', {\n          params: params\n        }).then(response => {\n          console.log(response.data);\n          this.forms.unit.data = response.data['unit_names'];\n        });\n      }\n      if (k == 'unloading_location') {\n        this.forms.unloading_location.name = name;\n        this.forms.unloading_location.value = name;\n      }\n      if (k == 'unit') {\n        this.forms.unit.name = name;\n        this.forms.unit.value = name;\n      }\n    },\n    async unloaded() {\n      const params = {\n        \"license_number\": this.forms.lic_number.value,\n        \"unloading_location\": this.forms.unloading_location.value,\n        \"unit\": this.forms.unit.value,\n        \"quantity\": this.forms.Quantity.value.replace(/,/g, ''),\n        \"quality\": this.forms.Quality.value,\n        \"forklift_driver\": this.forms.forklift_driver.value\n      };\n      const response = await this.axios.post('/myapp/api/unload', {}, {\n        params: params\n      });\n      console.log(response.data); // Access response data\n      if (response.data['status'] == 'success') {\n        this.success = true;\n      } else {\n        this.error = true;\n        this.errors = response.data['errors'];\n      }\n    }\n  }\n};","map":{"version":3,"names":["initFlowbite","modal","Alert","Card","ModalButton","Input","Dropdown","name","components","data","forms","lic_number","type","title","value","unloading_location","unit","Quantity","numbertype","lable","Quality","forklift_driver","success","error","errors","computed","formattedValue","formatNumber","inputValue","mounted","params","axios","post","then","response","console","log","get","watch","c","p","setTimeout","location","reload","methods","replace","formatInput","clicked","k","unloaded"],"sources":["C:\\Users\\Amir\\ISMv988\\ISMv900\\frontend\\src\\components\\forkliftPages\\unloaded.vue"],"sourcesContent":["<script>\r\nimport {initFlowbite} from \"flowbite\";\r\nimport modal from \"@/components/Modal.vue\";\r\nimport Alert from \"@/components/Alert.vue\";\r\nimport Card from \"@/components/Card.vue\";\r\nimport ModalButton from \"@/components/custom/ModalButton.vue\";\r\nimport Input from \"@/components/custom/Input.vue\";\r\nimport Dropdown from \"@/components/custom/Dropdown.vue\";\r\n\r\nexport default {\r\n  name: \"unloaded\",\r\n  components: {Dropdown, Input, ModalButton, Card, Alert, modal},\r\n  data(){\r\n    return {\r\n      forms: {\r\n        lic_number: {type: 'dropdown', name: 'شماره پلاک',title: 'شماره پلاک', data: '', value: ''},\r\n        unloading_location: {type: 'dropdown', name: 'محل تخلیه',title: 'محل تخلیه', data: '', value: ''},\r\n        unit: {type: 'dropdown', name: 'واحد',title: 'واحد', data: '', value: ''},\r\n        Quantity: {type:'input', name: 'مقدار',title: 'مقدار', value: '', numbertype:true, lable:'number'},\r\n        Quality: {type:'input', name: 'کیفیت',title: 'کیفیت', value: '', numbertype:true},\r\n        forklift_driver: {type:'input', name: 'اسم راننده لیفت تراک',title: 'اسم راننده لیفت تراک', value: ''},\r\n      },\r\n      success: false,\r\n      error: false,\r\n      errors: [],\r\n    }\r\n  },\r\n  computed: {\r\n    formattedValue() {\r\n      return this.formatNumber(this.inputValue);\r\n    },\r\n  },\r\n  mounted() {\r\n    initFlowbite();\r\n    const params = {\r\n      \"status\": 'LoadingUnloading',\r\n      \"location\": 'Weight1',\r\n      \"shipment_type\": 'Incoming',\r\n    }\r\n    this.axios.post('/myapp/api/getShipmentLicenseNumbers', {}, {params: params}).then((response) => {\r\n      console.log('lics:',response.data)\r\n      this.forms.lic_number.data = response.data['license_numbers']\r\n    })\r\n    this.axios.get('/myapp/api/getAnbarTableNames').then((response) => {\r\n      console.log(response.data)\r\n      this.forms.unloading_location.data = response.data['data']\r\n    })\r\n  },\r\n  watch:{\r\n    success(c, p){\r\n      if (c == true) {\r\n        setTimeout(() => {\r\n          this.success = false\r\n          location.reload();\r\n        }, 5000)\r\n      }\r\n    },\r\n  },\r\n  methods:{\r\n    formatNumber(value) {\r\n      return value.replace(/\\D/g, \"\").replace(/\\B(?=(\\d{3})+(?!\\d))/g, \",\");\r\n    },\r\n    formatInput() {\r\n      this.forms.Quantity.value = this.formatNumber(this.forms.Quantity.value);\r\n      // this.forms.Quality.value = this.formatNumber(this.forms.Quality.value);\r\n    },\r\n    clicked(k, name){\r\n      console.log(k, name)\r\n      if (k == 'lic_number'){\r\n        this.forms.lic_number.name = name\r\n        this.forms.lic_number.value = name\r\n        const params = {\r\n          'lic_number': this.forms.lic_number.value,\r\n        }\r\n        this.axios.get('/myapp/api/getUnitNamesBasedOnLicenseOfShipment', {params:params}).then((response) => {\r\n          console.log(response.data)\r\n          this.forms.unit.data = response.data['unit_names']\r\n        })\r\n      }\r\n      if (k == 'unloading_location'){\r\n        this.forms.unloading_location.name = name\r\n        this.forms.unloading_location.value = name\r\n      }\r\n      if (k == 'unit'){\r\n        this.forms.unit.name = name\r\n        this.forms.unit.value = name\r\n      }\r\n    },\r\n    async unloaded() {\r\n      const params = {\r\n        \"license_number\": this.forms.lic_number.value,\r\n        \"unloading_location\": this.forms.unloading_location.value,\r\n        \"unit\": this.forms.unit.value,\r\n        \"quantity\": this.forms.Quantity.value.replace(/,/g, ''),\r\n        \"quality\": this.forms.Quality.value,\r\n        \"forklift_driver\": this.forms.forklift_driver.value\r\n      };\r\n      const response = await this.axios.post('/myapp/api/unload', {}, {params: params})\r\n      console.log(response.data); // Access response data\r\n      if (response.data['status'] == 'success'){\r\n        this.success = true\r\n      }else {\r\n        this.error = true\r\n        this.errors = response.data['errors']\r\n      }\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n<form class=\"flex flex-col items-center mt-5 gap-4\">\r\n      <div v-if=\"error\" class=\"flex p-4 mb-4 text-sm text-red-800 rounded-lg bg-red-50 dark:bg-gray-800 dark:text-red-400\" role=\"alert\">\r\n            <svg class=\"flex-shrink-0 inline w-4 h-4 me-3 mt-[2px]\" aria-hidden=\"true\" xmlns=\"http://www.w3.org/2000/svg\" fill=\"currentColor\" viewBox=\"0 0 20 20\">\r\n              <path d=\"M10 .5a9.5 9.5 0 1 0 9.5 9.5A9.51 9.51 0 0 0 10 .5ZM9.5 4a1.5 1.5 0 1 1 0 3 1.5 1.5 0 0 1 0-3ZM12 15H8a1 1 0 0 1 0-2h1v-3H8a1 1 0 0 1 0-2h2a1 1 0 0 1 1 1v4h1a1 1 0 0 1 0 2Z\"/>\r\n            </svg>\r\n            <span class=\"sr-only\">Danger</span>\r\n            <div>\r\n              <span class=\"font-medium\">خطا! لطفا اطلاعات را برسی کنید:</span>\r\n                <ul class=\"mt-1.5 list-disc list-inside\">\r\n                  <li v-for=\"error in errors\">{{ error.message }}</li>\r\n              </ul>\r\n            </div>\r\n          </div>\r\n      <div v-if=\"success\" class=\"flex p-4 mb-4 text-sm text-green-800 rounded-lg bg-green-50 dark:bg-gray-800 dark:text-green-400\" role=\"alert\">\r\n        <svg class=\"flex-shrink-0 inline w-4 h-4 me-3 mt-[2px]\" aria-hidden=\"true\" xmlns=\"http://www.w3.org/2000/svg\" fill=\"currentColor\" viewBox=\"0 0 20 20\">\r\n          <path d=\"M10 .5a9.5 9.5 0 1 0 9.5 9.5A9.51 9.51 0 0 0 10 .5ZM9.5 4a1.5 1.5 0 1 1 0 3 1.5 1.5 0 0 1 0-3ZM12 15H8a1 1 0 0 1 0-2h1v-3H8a1 1 0 0 1 0-2h2a1 1 0 0 1 1 1v4h1a1 1 0 0 1 0 2Z\"/>\r\n        </svg>\r\n        <div>\r\n          <span class=\"font-medium\">\r\n            {{forms.unit.value}} {{forms.Quantity.value}}\r\n            با موفقیت به پلاگ {{forms.lic_number.value}} اضافه شد\r\n            <template v-for=\"(val, key) in forms\">\r\n                <p v-if=\"key=='reel_numbers'\">شماره رول:</p>\r\n                <ul v-if=\"key=='reel_numbers'\">\r\n                  <li v-for=\"item in val.value\" :key=\"item\">{{ item }}</li>\r\n                </ul>\r\n                <p v-else>{{val.title}}: {{val.value}}</p>\r\n            </template>\r\n          </span>\r\n        </div>\r\n      </div>\r\n      <template v-for=\"(val, form_name) in forms\">\r\n          <template v-if=\"val.type=='input'\">\r\n            <Input\r\n              :formName=\"form_name\"\r\n              :label=\"val.name\"\r\n              :type=\"val.lable\"\r\n              :disabled=\"val.disabled\"\r\n              @update=\"val.value = $event\"\r\n              :value=\"val.value\"\r\n              @InputError=\"error = $event\"\r\n            />\r\n        </template>\r\n          <template v-if=\"val.type=='dropdown'\">\r\n          <Dropdown :formName=\"form_name\">\r\n            <template v-slot:btnName>\r\n              {{val.name}}\r\n            </template>\r\n            <template v-slot:list>\r\n              <li v-for=\"(data, index) in val.data\" :key=\"index\">\r\n                <a @click=\"clicked(form_name, data)\" type=\"button\" class=\"block px-4 py-2 hover:bg-gray-100 dark:hover:bg-gray-600 dark:hover:text-white\">\r\n                  {{ data }}\r\n                </a>\r\n              </li>\r\n            </template>\r\n          </Dropdown>\r\n        </template>\r\n\r\n<!--        <template v-if=\"val.type=='input'\">-->\r\n<!--          <div class=\"relative\">-->\r\n<!--            <template v-if=\"val.numbertype\">-->\r\n<!--              <input v-model=\"val.value\" @input=\"formatInput\" type=\"text\" :id=\"form_name\" :class=\"[val.error ? 'text-red-900 border-red-500 focus:border-red-500' : 'text-gray-900 focus:border-green-500 border-gray-300']\" class=\"block px-2.5 pb-2.5 pt-4 w-full text-sm  bg-transparent rounded-lg border-1 appearance-none focus:outline-none focus:ring-0 peer\" placeholder=\"\" />-->\r\n<!--            </template>-->\r\n<!--            <template v-else>-->\r\n<!--              <input v-model=\"val.value\" type=\"text\" :id=\"form_name\" :class=\"[val.error ? 'text-red-900 border-red-500 focus:border-red-500' : 'text-gray-900 focus:border-green-500 border-gray-300']\" class=\"block px-2.5 pb-2.5 pt-4 w-full text-sm  bg-transparent rounded-lg border-1 appearance-none focus:outline-none focus:ring-0 peer\" placeholder=\"\" />-->\r\n<!--            </template>-->\r\n<!--            <label :for=\"form_name\" :class=\"[val.error ? 'peer-focus:text-red-500 text-red-500' : 'peer-focus:text-green-500 text-gray-500']\" class=\"absolute text-sm dark:text-gray-400 duration-300 transform -translate-y-4 scale-75 top-2 z-10 origin-[0] bg-white dark:bg-gray-900 px-2 peer-focus:px-2  peer-focus:dark:text-blue-500 peer-placeholder-shown:scale-100 peer-placeholder-shown:-translate-y-1/2 peer-placeholder-shown:top-1/2 peer-focus