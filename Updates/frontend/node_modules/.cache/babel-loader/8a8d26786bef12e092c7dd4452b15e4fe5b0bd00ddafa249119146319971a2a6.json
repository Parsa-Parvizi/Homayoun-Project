{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { initFlowbite } from \"flowbite\";\nimport modal from \"@/components/Modal.vue\";\nimport Alert from \"@/components/Alert.vue\";\nimport Card from \"@/components/Card.vue\";\nimport Input from \"@/components/custom/Input.vue\";\nimport Dropdown from \"@/components/custom/Dropdown.vue\";\nimport ModalButton from \"@/components/custom/ModalButton.vue\";\nexport default {\n  name: \"addNewMatrialType\",\n  components: {\n    ModalButton,\n    Dropdown,\n    Input,\n    Card,\n    Alert,\n    modal\n  },\n  data() {\n    return {\n      forms: {\n        supplier_name: {\n          type: 'dropdown',\n          name: 'فروشنده',\n          title: 'فروشنده',\n          data: '',\n          value: '',\n          label: 'text'\n        },\n        matrial_type: {\n          type: 'input',\n          name: 'نوع متریال جدید',\n          title: 'نوع متریال جدید',\n          value: '',\n          label: 'text'\n        },\n        username: {\n          type: 'input',\n          name: 'نام کاربر',\n          title: 'نام کاربر',\n          value: '',\n          label: 'text'\n        }\n      },\n      success: false,\n      error: false,\n      errors: []\n    };\n  },\n  mounted() {\n    initFlowbite();\n    this.getSupplierName();\n  },\n  watch: {\n    success(c, p) {\n      if (c == true) {\n        setTimeout(() => {\n          this.success = false;\n          location.reload();\n        }, 5000);\n      }\n    }\n  },\n  methods: {\n    getSupplierName() {\n      this.axios.get('/myapp/api/getSupplierNames').then(response => {\n        // console.log(response.data)\n        this.forms.supplier_name.data = response.data['supplier_names'];\n      });\n    },\n    clicked(k, name) {\n      console.log(k, name);\n      if (k == 'supplier_name') {\n        this.forms.supplier_name.name = name;\n        this.forms.supplier_name.value = name;\n      }\n    },\n    async addNewMatrialType() {\n      const params = {\n        \"supplier_name\": this.forms.supplier_name.value,\n        \"material_type\": this.forms.matrial_type.value,\n        \"username\": this.forms.username.value\n      };\n      this.errors = [];\n      for (const key in this.forms) {\n        if (this.forms[key].value == '') {\n          this.forms[key].error = true;\n          this.errors.push({\n            'message': `${this.forms[key].name} مورد نیاز است`\n          });\n        } else {\n          this.forms[key].error = false;\n        }\n      }\n      if (this.errors.length == 0) {\n        this.error = false;\n        const response = await this.axios.post('/myapp/addMaterialType/', {}, {\n          params: params\n        });\n        console.log(response.data); // Access response data\n        if (response.data['status'] == 'success') {\n          this.success = true;\n        } else {\n          this.error = true;\n          this.errors = response.data['errors'];\n        }\n      } else {\n        this.error = true;\n      }\n    }\n  }\n};","map":{"version":3,"names":["initFlowbite","modal","Alert","Card","Input","Dropdown","ModalButton","name","components","data","forms","supplier_name","type","title","value","label","matrial_type","username","success","error","errors","mounted","getSupplierName","watch","c","p","setTimeout","location","reload","methods","axios","get","then","response","clicked","k","console","log","addNewMatrialType","params","key","push","length","post"],"sources":["C:\\Users\\Amir\\ISMv988\\ISMv900\\frontend\\src\\components\\admin\\addNewMatrialType.vue"],"sourcesContent":["<script>\r\nimport {initFlowbite} from \"flowbite\";\r\nimport modal from \"@/components/Modal.vue\";\r\nimport Alert from \"@/components/Alert.vue\";\r\nimport Card from \"@/components/Card.vue\";\r\nimport Input from \"@/components/custom/Input.vue\";\r\nimport Dropdown from \"@/components/custom/Dropdown.vue\";\r\nimport ModalButton from \"@/components/custom/ModalButton.vue\";\r\n\r\nexport default {\r\n  name: \"addNewMatrialType\",\r\n  components: {ModalButton, Dropdown, Input, Card, Alert, modal},\r\n  data(){\r\n    return {\r\n      forms: {\r\n        supplier_name: {type: 'dropdown', name: 'فروشنده',title: 'فروشنده', data: '', value: '', label:'text'},\r\n        matrial_type: {type:'input', name: 'نوع متریال جدید',title: 'نوع متریال جدید', value: '', label:'text'},\r\n        username: {type:'input', name: 'نام کاربر',title: 'نام کاربر', value: '' , label:'text'},\r\n      },\r\n      success: false,\r\n      error: false,\r\n      errors: [],\r\n    }\r\n  },\r\n  mounted() {\r\n    initFlowbite();\r\n    this.getSupplierName()\r\n  },\r\n  watch:{\r\n    success(c, p){\r\n      if (c == true) {\r\n        setTimeout(() => {\r\n          this.success = false\r\n          location.reload();\r\n        }, 5000)\r\n      }\r\n    },\r\n  },\r\n  methods:{\r\n    getSupplierName(){\r\n      this.axios.get('/myapp/api/getSupplierNames').then((response) => {\r\n        // console.log(response.data)\r\n        this.forms.supplier_name.data = response.data['supplier_names']\r\n      })\r\n    },\r\n    clicked(k, name){\r\n      console.log(k, name)\r\n      if (k == 'supplier_name'){\r\n        this.forms.supplier_name.name = name\r\n        this.forms.supplier_name.value = name\r\n      }\r\n    },\r\n    async addNewMatrialType() {\r\n      const params = {\r\n        \"supplier_name\": this.forms.supplier_name.value,\r\n        \"material_type\": this.forms.matrial_type.value,\r\n        \"username\": this.forms.username.value,\r\n      };\r\n      this.errors = []\r\n      for (const key in this.forms) {\r\n        if (this.forms[key].value == ''){\r\n          this.forms[key].error = true\r\n          this.errors.push({'message': `${this.forms[key].name} مورد نیاز است`})\r\n        }else {\r\n           this.forms[key].error = false\r\n        }\r\n      }\r\n      if (this.errors.length == 0){\r\n        this.error = false\r\n        const response = await this.axios.post('/myapp/addMaterialType/', {}, {params: params})\r\n        console.log(response.data); // Access response data\r\n        if (response.data['status'] == 'success'){\r\n          this.success = true\r\n        }else {\r\n          this.error = true\r\n          this.errors = response.data['errors']\r\n        }\r\n      } else {\r\n        this.error = true\r\n      }\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n  <Card title=\"افزودن نوع جنس\">\r\n    <div class=\"flex flex-col gap-2 justify-center items-center\">\r\n      <div v-if=\"error\" class=\"flex p-4 mb-4 text-sm text-red-800 rounded-lg bg-red-50 dark:bg-gray-800 dark:text-red-400\" role=\"alert\">\r\n            <svg class=\"flex-shrink-0 inline w-4 h-4 me-3 mt-[2px]\" aria-hidden=\"true\" xmlns=\"http://www.w3.org/2000/svg\" fill=\"currentColor\" viewBox=\"0 0 20 20\">\r\n              <path d=\"M10 .5a9.5 9.5 0 1 0 9.5 9.5A9.51 9.51 0 0 0 10 .5ZM9.5 4a1.5 1.5 0 1 1 0 3 1.5 1.5 0 0 1 0-3ZM12 15H8a1 1 0 0 1 0-2h1v-3H8a1 1 0 0 1 0-2h2a1 1 0 0 1 1 1v4h1a1 1 0 0 1 0 2Z\"/>\r\n            </svg>\r\n            <span class=\"sr-only\">Danger</span>\r\n            <div>\r\n              <span class=\"font-medium\">خطا! لطفا اطلاعات را برسی کنید:</span>\r\n                <ul class=\"mt-1.5 list-disc list-inside\">\r\n                  <li v-for=\"error in errors\">{{ error.message }}</li>\r\n              </ul>\r\n            </div>\r\n          </div>\r\n      <div v-if=\"success\" class=\"flex p-4 mb-4 text-sm text-green-800 rounded-lg bg-green-50 dark:bg-gray-800 dark:text-green-400\" role=\"alert\">\r\n        <svg class=\"flex-shrink-0 inline w-4 h-4 me-3 mt-[2px]\" aria-hidden=\"true\" xmlns=\"http://www.w3.org/2000/svg\" fill=\"currentColor\" viewBox=\"0 0 20 20\">\r\n          <path d=\"M10 .5a9.5 9.5 0 1 0 9.5 9.5A9.51 9.51 0 0 0 10 .5ZM9.5 4a1.5 1.5 0 1 1 0 3 1.5 1.5 0 0 1 0-3ZM12 15H8a1 1 0 0 1 0-2h1v-3H8a1 1 0 0 1 0-2h2a1 1 0 0 1 1 1v4h1a1 1 0 0 1 0 2Z\"/>\r\n        </svg>\r\n        <div>\r\n          <span class=\"font-medium\">\r\n            جنس ماده جدید با نام {{ forms.matrial_type.value }} با موفقیت به سیستم اضافه شد.\r\n            <template v-for=\"(val, key) in forms\">\r\n                <p v-if=\"key=='reel_numbers'\">شماره رول:</p>\r\n                <ul v-if=\"key=='reel_numbers'\">\r\n                  <li v-for=\"item in val.value\" :key=\"item\">{{ item }}</li>\r\n                </ul>\r\n                <p v-else>{{val.title}}: {{val.value}}</p>\r\n            </template>\r\n          </span>\r\n        </div>\r\n      </div>\r\n      <template v-for=\"(val, form_name) in forms\">\r\n        <template v-if=\"val.type=='input'\">\r\n            <Input\r\n              :formName=\"form_name\"\r\n              :label=\"val.name\"\r\n              :type=\"val.lable\"\r\n              :disabled=\"val.disabled\"\r\n              @update=\"val.value = $event\"\r\n              @InputError=\"error = $event\"\r\n            />\r\n        </template>\r\n        <template v-if=\"val.type=='dropdown'\">\r\n          <Dropdown :formName=\"form_name\">\r\n            <template v-slot:btnName>\r\n              {{val.name}}\r\n            </template>\r\n            <template v-slot:list>\r\n              <li v-for=\"(data, index) in val.data\" :key=\"index\">\r\n                <a @click=\"clicked(form_name, data)\" type=\"button\" class=\"block px-4 py-2 hover:bg-gray-100 dark:hover:bg-gray-600 dark:hover:text-white\">\r\n                  {{ data }}\r\n                </a>\r\n              </li>\r\n            </template>\r\n          </Dropdown>\r\n        </template>\r\n      </template>\r\n      <modal type=\"confirm\" :disabled=\"error\">\r\n        <template v-slot:button>اضافه کردن</template>\r\n        <template v-slot:text>\r\n          <div class=\"flex flex-col gap-2 font-bold\">\r\n            <template v-for=\"(val, key) in forms\">\r\n                <p>{{val.title}}: {{val.value}}</p>\r\n            </template>\r\n          </div>\r\n        </template>\r\n        <template v-slot:btns>\r\n          <div>\r\n            <ModalButton @close=\"addNewMatrialType\" />\r\n          </div>\r\n        </template>\r\n      </modal>\r\n    </div>\r\n  </Card>\r\n</template>"],"mappings":";AACA,SAAQA,YAAY,QAAO,UAAU;AACrC,OAAOC,KAAI,MAAO,wBAAwB;AAC1C,OAAOC,KAAI,MAAO,wBAAwB;AAC1C,OAAOC,IAAG,MAAO,uBAAuB;AACxC,OAAOC,KAAI,MAAO,+BAA+B;AACjD,OAAOC,QAAO,MAAO,kCAAkC;AACvD,OAAOC,WAAU,MAAO,qCAAqC;AAE7D,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,UAAU,EAAE;IAACF,WAAW;IAAED,QAAQ;IAAED,KAAK;IAAED,IAAI;IAAED,KAAK;IAAED;EAAK,CAAC;EAC9DQ,IAAIA,CAAA,EAAE;IACJ,OAAO;MACLC,KAAK,EAAE;QACLC,aAAa,EAAE;UAACC,IAAI,EAAE,UAAU;UAAEL,IAAI,EAAE,SAAS;UAACM,KAAK,EAAE,SAAS;UAAEJ,IAAI,EAAE,EAAE;UAAEK,KAAK,EAAE,EAAE;UAAEC,KAAK,EAAC;QAAM,CAAC;QACtGC,YAAY,EAAE;UAACJ,IAAI,EAAC,OAAO;UAAEL,IAAI,EAAE,iBAAiB;UAACM,KAAK,EAAE,iBAAiB;UAAEC,KAAK,EAAE,EAAE;UAAEC,KAAK,EAAC;QAAM,CAAC;QACvGE,QAAQ,EAAE;UAACL,IAAI,EAAC,OAAO;UAAEL,IAAI,EAAE,WAAW;UAACM,KAAK,EAAE,WAAW;UAAEC,KAAK,EAAE,EAAC;UAAIC,KAAK,EAAC;QAAM;MACzF,CAAC;MACDG,OAAO,EAAE,KAAK;MACdC,KAAK,EAAE,KAAK;MACZC,MAAM,EAAE;IACV;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRrB,YAAY,CAAC,CAAC;IACd,IAAI,CAACsB,eAAe,CAAC;EACvB,CAAC;EACDC,KAAK,EAAC;IACJL,OAAOA,CAACM,CAAC,EAAEC,CAAC,EAAC;MACX,IAAID,CAAA,IAAK,IAAI,EAAE;QACbE,UAAU,CAAC,MAAM;UACf,IAAI,CAACR,OAAM,GAAI,KAAI;UACnBS,QAAQ,CAACC,MAAM,CAAC,CAAC;QACnB,CAAC,EAAE,IAAI;MACT;IACF;EACF,CAAC;EACDC,OAAO,EAAC;IACNP,eAAeA,CAAA,EAAE;MACf,IAAI,CAACQ,KAAK,CAACC,GAAG,CAAC,6BAA6B,CAAC,CAACC,IAAI,CAAEC,QAAQ,IAAK;QAC/D;QACA,IAAI,CAACvB,KAAK,CAACC,aAAa,CAACF,IAAG,GAAIwB,QAAQ,CAACxB,IAAI,CAAC,gBAAgB;MAChE,CAAC;IACH,CAAC;IACDyB,OAAOA,CAACC,CAAC,EAAE5B,IAAI,EAAC;MACd6B,OAAO,CAACC,GAAG,CAACF,CAAC,EAAE5B,IAAI;MACnB,IAAI4B,CAAA,IAAK,eAAe,EAAC;QACvB,IAAI,CAACzB,KAAK,CAACC,aAAa,CAACJ,IAAG,GAAIA,IAAG;QACnC,IAAI,CAACG,KAAK,CAACC,aAAa,CAACG,KAAI,GAAIP,IAAG;MACtC;IACF,CAAC;IACD,MAAM+B,iBAAiBA,CAAA,EAAG;MACxB,MAAMC,MAAK,GAAI;QACb,eAAe,EAAE,IAAI,CAAC7B,KAAK,CAACC,aAAa,CA